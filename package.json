{
  "name": "kintone-plugin-unrelated-lookup",
  "version": "1.9.2",
  "author": "ribbit",
  "license": "MIT",
  "scripts": {
    "init": "node scripts/marge-json.mjs dev && kintone-plugin-packer --out dist/plugin-dev.zip plugin && node scripts/rename-ppk.mjs",
    "build": "node scripts/marge-json.mjs prod && kbuild prod plugin --prod-root cdn && kintone-plugin-packer --out dist/plugin.zip --ppk dist/private.ppk plugin",
    "standalone": "node scripts/marge-json.mjs standalone && kbuild prod plugin --prod-root plugin && kintone-plugin-packer --out dist/plugin-standalone.zip --ppk dist/private.ppk plugin",
    "dev": "node scripts/marge-json.mjs dev && kintone-plugin-packer --out dist/plugin-dev.zip --ppk dist/private.ppk plugin && concurrently \"yarn:dev:*\"",
    "dev:build": "kbuild dev plugin",
    "dev:upload": "node scripts/upload.mjs",
    "lint": "eslint ./src --cache --ext .js,.ts,.tsx"
  },
  "devDependencies": {
    "@emotion/css": "11.11.2",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@kintone/dts-gen": "7.0.4",
    "@kintone/plugin-packer": "7.0.4",
    "@kintone/plugin-uploader": "8.0.0",
    "@kintone/rest-api-client": "4.1.0",
    "@kintone/webpack-plugin-kintone-plugin": "7.0.4",
    "@konomi-app/kbuild": "^2.0.0",
    "@konomi-app/kintone-utilities": "^3.0.0",
    "@konomi-app/kintone-utility-component": "^1.2.0",
    "@lb-ribbit/kintone-xapp": "0.4.0",
    "@mui/icons-material": "5.14.8",
    "@mui/material": "5.14.8",
    "@types/dompurify": "3.0.2",
    "@types/luxon": "3.3.2",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "6.6.0",
    "@typescript-eslint/parser": "6.6.0",
    "concurrently": "^8.2.1",
    "dompurify": "3.0.5",
    "dotenv": "16.3.1",
    "esbuild-loader": "4.0.2",
    "eslint": "8.49.0",
    "fork-ts-checker-webpack-plugin": "8.0.0",
    "immer": "10.0.2",
    "luxon": "3.4.3",
    "notistack": "3.0.1",
    "npm-run-all": "4.1.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.11",
    "recoil": "0.7.7",
    "ts-loader": "^9.4.4",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  },
  "prettier": {
    "printWidth": 100,
    "arrowParens": "always",
    "singleQuote": true,
    "jsxSingleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "endOfLine": "lf"
  }
}
